    "turndown": "^7.2.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/turndown": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
